    "docz": "^2.0.0-rc.66",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "tmp": "^0.0.33",
    "ts-jest": "^24.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.20.0",
