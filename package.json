{
  "name": "@theuiteam/lib-builder",
  "version": "0.3.0",
  "description": "Opinionated toolbox boilerplate to create Typescript React components",
  "main": "dist/index.js",
  "repository": "https://github.com/theuiteam/lib-builder",
  "license": "MIT",
  "bin": {
    "lib-builder": "./bin/lib-builder"
  },
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "add_permission": "chmod +x ./bin/lib-builder",
    "lint": "eslint src/**/*.ts --fix",
    "cli": "./bin/lib-builder",
    "prepack": "yarn add_permission && yarn build",
    "release": "npm publish --access public",
    "prepare": "husky install"
  },
  "dependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "chalk": "^2.3.2",
    "codecov": "^3.6.1",
    "conventional-changelog-cli": "^2.0.28",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^7.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^13.1.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "size-limit": "^11.1.6",
    "tmp": "^0.0.33",
    "ts-jest": "^29.2.5",
    "tslib": "^2.0.0",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist",
    "bin",
    "configs",
    "static"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint -c static/eslint.config.mjs --fix"
    ],
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "^20.10.2",
    "@types/tmp": "^0.0.33"
  },
  "packageManager": "yarn@1.22.19"
}
